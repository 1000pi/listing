package listing

import (
	"testing"
)

func TestPermIntList(t *testing.T) {
	for perm := range Permute(IntList{1, 2, 3, 6}, 3, 5) {
		t.Log(perm)
	}
}

func TestCombIntlist(t *testing.T) {
	for conb := range Combination(IntList{24, 54, 75, 39, 47}, 3, 5) {
		t.Log(conb)
	}
}

func TestPermRuneList(t *testing.T) {
	for perm := range Permute(RuneList([]rune("aiue")), 3, 5) {
		t.Log(string(perm.(RuneList)))
	}
}
